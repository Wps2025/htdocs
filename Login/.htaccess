# Bloq.config-início
<Files "config.php">
    Order Allow,Deny
    Deny from all
</Files>
# Fechamento.Bloq.config

# Quando houver error 403 p/Mtv-Bloq.config= irá direcionar para página Home
ErrorDocument 403 http://localhost/projeto_Cadidatos/projeto_Cadidatos/Login/home.php

#Bloq.os arquivos de listagem
#Options - Indexs

# BEGIN WordPress
<IfModule mod_rewrite.c>
RewriteEngine On
RewriteBase /
RewriteRule ^index.php$ - [L]
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule . /index.php [L]
</IfModule>
# END WordPress


